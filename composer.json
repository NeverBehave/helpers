{
    "name": "neverbehave/lumen-helpers",
    "type": "library",
    "description": "generic helpers for Lumen",
    "keywords": [
        "NeverBehave",
        "helpers",
        "Lumen"
    ],
    "homepage": "https://github.com/NeverBehave/lumen-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "neverbehave",
            "email": "i@never.pet",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/console": "5.5.*|5.6.*|5.7.*",
        "illuminate/contracts": "5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0|~7.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "suggest": {
        "moneyphp/money": "Money value object."
    },
    "autoload": {
        "psr-4": {
            "neverbehave\\helpers\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "neverbehave\\helpers\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "neverbehave\\helpers\\HelperServiceProvider"
            ]
        }
    }
}
